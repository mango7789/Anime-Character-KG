{
    "entity_types": {
        "Work": {
            "description": "Creative works such as animation, manga, novels, and games",
            "examples": [
                "A Song of Ice and Fire",
                "Jujutsu Kaisen"
            ]
        },
        "Character": {
            "description": "Fictional characters appearing in works",
            "examples": [
                "Eren Yeager",
                "Rem"
            ]
        },
        "Person": {
            "description": "Real-world individuals such as authors, voice actors, and directors",
            "examples": [
                "Hajime Isayama",
                "Natsuki Hanae"
            ]
        },
        "Organization": {
            "description": "Real-world organizations such as production companies and committees",
            "examples": [
                "WIT STUDIO",
                "MAPPA"
            ]
        },
        "Group": {
            "description": "In-universe organizations or groups that characters belong to",
            "examples": [
                "Survey Corps",
                "Class 1-A",
                "Akatsuki"
            ]
        },
        "Location": {
            "description": "In-universe locations appearing in fictional works",
            "examples": [
                "Underground City",
                "Shiganshina District"
            ]
        }
    },
    "attribute_types": {
        "Time": {
            "description": "Time point or time span",
            "examples": [
                "2013-04-06",
                "December 25"
            ]
        },
        "Literal": {
            "description": "Plain text values",
            "examples": [
                "Male",
                "Alive"
            ]
        },
        "Numeric": {
            "description": "Numeric values or numeric ranges",
            "examples": [
                "160",
                "34-38"
            ]
        },
        "Tag": {
            "description": "Descriptive tags for characters or works",
